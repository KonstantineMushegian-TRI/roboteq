<launch>
    <arg name="dual" default="true" />
    <node pkg="roboteq_driver" type="driver_node" name="roboteq_driver" output="screen">
        <rosparam>
            port: /dev/ttyUSB0
            baud: 115200
        </rosparam>
        <rosparam  if="$(arg dual)">
            channels: ['fwdrv', 'lr']
        </rosparam>
    </node>
</launch>

